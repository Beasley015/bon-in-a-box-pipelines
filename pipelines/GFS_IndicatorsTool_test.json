  "inputs": {
    "pipeline@102": {
      "label": "Title of the run",
      "description": "Set a name for the pipeline run, for instance indicating species of interest, region, type of habitat map, and monitoring period. ",
      "type": "text",
      "example": "Quercus sartorii, Mexico, Habitat decline by tree cover loss, 2000-2023",
      "weight": 0
    },
    "pipeline@103": {
      "label": "Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided, the first one will be used in figures. ",
      "type": "float[]",
      "example": [
        50,
        100,
        1000
      ],
      "weight": 1
    },
    "pipeline@104": {
      "label": "Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided, the first one will be used in figure.",
      "type": "float[]",
      "example": [
        0.05,
        0.1,
        0.5
      ],
      "weight": 2
    },
    "pipeline@105": {
      "label": "Species names",
      "description": "Scientific name of the species, used to look for occurrences in GBIF. ",
      "type": "text",
      "example": "Quercus sartorii",
      "weight": 3
    },
    "pipeline@106": {
      "label": "Countries list",
      "description": "countries of interest, will be used to look for GBIF occurrences.",
      "type": "text[]",
      "example": [
        "Mexico",
        "Guatemala"
      ],
      "weight": 4
    },
    "pipeline@107": {
      "label": "Start year",
      "description": "Integer, 4 digit year, start date to retrieve occurrences",
      "type": "int",
      "example": 1980,
      "weight": 5
    },
    "pipeline@108": {
      "label": "End year",
      "description": "Integer, 4 digit year, end date to retrieve occurrences",
      "type": "int",
      "example": 2000,
      "weight": 6
    },
    "pipeline@109": {
      "label": "Size of buffer",
      "description": "Radius size [in km] to calculate buffer around species observations. [mandatory if Type of Population Input = \"species_obs\", \"countries\" or \"bbox\"; otherwise ignored]",
      "type": "float",
      "example": 10,
      "weight": 7
    },
    "pipeline@110": {
      "label": "Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations. [mandatory if Type of Population Input = \"species_obs\", \"countries\" or \"bbox\"; otherwise ignored]",
      "type": "float",
      "example": 50,
      "weight": 8
    }
  },
