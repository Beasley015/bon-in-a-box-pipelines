  "inputs": {
    "pipeline@102": {
      "label": "Title of the run",
      "description": "Set a name for the pipeline run, for instance indicating species of interest, region, type of habitat map, and monitoring period. ",
      "type": "text",
      "example": "Rhinanthus alectorolophus, Switzerland Grasslands, Shrublands and Sparse Vegetation, 1995-2020",
      "weight": 0
    },
    "pipeline@105": {
      "label": "Species names",
      "description": "Scientific name of the species, used to look for occurrences in GBIF. ",
      "type": "text",
      "example": "Rhinanthus alectorolophus",
      "weight": 1
    },
    "pipeline@107": {
      "label": "Start year occurrences",
      "description": "Integer, 4 digit year, start date to retrieve occurrences from GBIF.",
      "type": "int",
      "example": 1980,
      "weight": 2
    },
    "pipeline@108": {
      "label": "End year occurences",
      "description": "Integer, 4 digit year, end date to retrieve occurrences from GBIF.",
      "type": "int",
      "example": 1995,
      "weight": 3
    },
    "pipeline@116": {
      "label": "Years of monitoring land cover change",
      "description": "List of years for which land cover habitat change should be monitored (maximum range 1992 - 2020). The monitoring period should be set after the occurrence retrieval period. ",
      "type": "int[]",
      "example": [
        1995,
        2000,
        2005,
        2010,
        2015,
        2020
      ],
      "weight": 4
    },
    "pipeline@117": {
      "label": "Landcover classes",
      "description": "List of landcover class identifiers to be extract (for ex., 130 corresponds to \"grassland\", for complete list of identifiers see https://savs.eumetsat.int/html/images/landcover_legend.png). Multiple classes can be used, separated by comma. ",
      "type": "int[]",
      "example": [
        "120",
        "130",
        "150"
      ],
      "weight": 5
    },
    "pipeline@103": {
      "label": "Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided, the first one will be used in figures. ",
      "type": "float[]",
      "example": [
        "500",
        "1000"
      ],
      "weight": 6
    },
    "pipeline@118": {
      "label": "Resolution of the land cover map",
      "description": "Desired resolution for land cover map, will be obtained via resampling. To be specified in decimal degrees (0.01 ~ 1 km). Minimal value 0.003 (~300m).",
      "type": "float",
      "example": 0.01,
      "weight": 7
    },
    "pipeline@104": {
      "label": "Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided, the first one will be used in figure.",
      "type": "float[]",
      "example": [
        "0.1"
      ],
      "weight": 8
    },
    "pipeline@106": {
      "label": "Countries list",
      "description": "countries of interest, will be used to look for GBIF occurrences.",
      "type": "text[]",
      "example": [
        "Switzerland"
      ],
      "weight": 9
    },
    "pipeline@109": {
      "label": "Size of buffer",
      "description": "Radius size [in km] to calculate buffer around species observations.",
      "type": "float",
      "example": 5,
      "weight": 10
    },
    "pipeline@110": {
      "label": "Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations. ",
      "type": "float",
      "example": 25,
      "weight": 11
    }
  },
  "outputs": {
    "GFS_Indicators.json@100|GFS_IndicatorsTool>get_Indicators.yml@104|Interactive_plot": {
      "description": "An interactive interface to explore indicators trends across geographical space and time.",
      "label": "Interactive plot",
      "weight": 0,
      "type": "text/html"
    }
  },
  "metadata": {
    "name": "GFS TOOL - Land cover loss by populations from GBIF occurrences (country)",
    "description": "Genes from Space tool. Retrieves species occurrence from GBIF, calculates polygons of population distribution, and computes indices of genetic diversity monitoring for land cover habitat loss.",
    "author": [
      {
        "name": "Oliver Selmoni",
        "email": "oliver.selmoni@gmail.com",
        "identifier": null
      }
    ],
    "license": null,
    "external_link": "https://teams.issibern.ch/genesfromspace/",
    "references": [
      {
        "text": "Schuman et al., EcoEvoRxiv.",
        "doi": "https://doi.org/10.32942/X2RS58"
      },
      {
        "text": "ESA. Land Cover CCI Product User Guide Version 2. Tech. Rep. (2017)",
        "doi": "https://maps.elie.ucl.ac.be/CCI/viewer/download/ESACCI-LC-Ph2-PUGv2_2.0.pdf"
      }
    ]
  }
