  "inputs": {
    "pipeline@59": {
      "label": "1 - Type of population input",
      "description": "Different options are available to identify the spatial distribution of populations. If spatial polygons describing the geographic distribution of populations are available, select \"population_polygons\". Alternatively, populations can be estimated from observation points of the species of interest. If such points are available, select \"species_obs\". If points are not available, the tool can retrieved species observation from GBIF for a user-defined bounding box (select \"bbox\") or for a list of countries of interest (select \"countries\")",
      "type": "options",
      "example": "countries",
      "options": [
        "countries",
        "bbox",
        "species_obs",
        "population_polygons"
      ],
      "weight": 0
    },
    "pipeline@60": {
      "label": "1a - Polygons of populations",
      "description": "Path to geojson file storing polygons of populations. Polygons must include an attribute \"pop\" specifying the identifier of each population. [mandatory if population input is \"population_polygons\"] ",
      "type": "text",
      "example": "/userdata/populations.geojson",
      "weight": 1
    },
    "pipeline@61": {
      "label": "1b - Coordinates of species occurrence",
      "description": "Path to table in tsv format, storing the species observation coordinates. The table must include a header with \"decimal_longitude\" and \"decimal_latitude\" columns, indicating the coordinates of every observation. [mandatory if population input is \"species_obs\"] ",
      "type": "text",
      "example": "/userdata/obs_data.tsv",
      "weight": 2
    },
    "pipeline@62": {
      "label": "1c - Bounding box",
      "description": "bounding box of area of interest, will be used to look for GBIF observations. [mandatory if population input is \"bbox\"] ",
      "type": "float[]",
      "example": [
        -99,
        22,
        -92,
        29
      ],
      "weight": 3
    },
    "pipeline@63": {
      "label": "1d - Countries list",
      "description": "countries of interest, will be used to look for GBIF observations. [mandatory if population input is \"countries\"] ",
      "type": "text[]",
      "example": [
        "Mexico",
        "Guatemala"
      ],
      "weight": 4
    },
    "pipeline@67": {
      "label": "2 - Species names",
      "description": "Scientific name of the species, will be used to retrieve observations from GBIF [mandatory if population input is  \"bbox\" or \"countries\"] ",
      "type": "text",
      "example": "Quercus sartorii",
      "weight": 5
    },
    "pipeline@68": {
      "label": "3a - Start year",
      "description": "Integer, 4 digit year, start date to retrieve occurrences in GBIF [mandatory if population input is  \"bbox\" or \"countries\"] ",
      "type": "int",
      "example": 1980,
      "weight": 6
    },
    "pipeline@69": {
      "label": "3b - End year",
      "description": "Integer, 4 digit year, end date to retrieve occurrences in GBIF [mandatory if population input is  \"bbox\" or \"countries\"] ",
      "type": "int",
      "example": 2000,
      "weight": 7
    },
    "pipeline@77": {
      "label": "4a - Size of population buffer",
      "description": "Radius size [in km] to calculate buffer around species observations.  [mandatory if population input is\"species_obs\", \"countries\" or \"bbox\"]",
      "type": "float",
      "example": 10,
      "weight": 8
    },
    "pipeline@78": {
      "label": "4b - Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations. [mandatory if population input is\"species_obs\", \"countries\" or \"bbox\"]",
      "type": "float",
      "example": 50,
      "weight": 9
    },
    "pipeline@84": {
      "label": "5 - Resolution of tree cover map",
      "description": "Desired resolution for tree cover map, will be obtained via resampling. To be specified in decimal degrees (0.01 ~ 1 km). Minimal value 0.001 (~100m).",
      "type": "float",
      "example": 0.01,
      "weight": 10
    },
    "pipeline@89": {
      "label": "6a - Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided.",
      "type": "float[]",
      "example": [
        "0.1",
        "0.2",
        "0.5"
      ],
      "weight": 11
    },
    "pipeline@90": {
      "label": "6b - Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided.",
      "type": "float[]",
      "example": [
        "500",
        "100",
        "1000"
      ],
      "weight": 12
    },
    "pipeline@97": {
      "label": "7 - Title of the run",
      "description": "Set a title summarizing the pipeline run (studied species, region, habitat loss variable, period). ",
      "type": "text",
      "example": "Quercus sartorii, Mexico, Habitat decline by tree cover loss, 2000-2023",
      "weight": 13
    }
  },
  "outputs": {
    "GFS_IndicatorsTool>get_Indicators.yml@88|Ne_table": {
      "label": "Effective population size",
      "type": "text/tab-separated-values",
      "weight": 0,
      "description": "Estimated effective size of every population, based on the latest time point of the habitat cover map. "
    },
    "GFS_IndicatorsTool>get_Indicators.yml@88|Ne_plot": {
      "label": "Effective population size over time",
      "type": "image/png",
      "weight": 1,
      "description": "Plot displaying change of effective population size over time. "
    },
    "GFS_IndicatorsTool>get_Indicators.yml@88|PM": {
      "label": "Population maintained",
      "type": "float",
      "weight": 2,
      "description": "Estimated proportion of maintained populations, comparing earliest and latest time point. "
    },
    "GFS_IndicatorsTool>get_Indicators.yml@88|PM_plot": {
      "label": "Populations maintained over time",
      "type": "image/png",
      "weight": 3,
      "description": "Plot showing the change in population area (absolute change and relative change) over time. "
    },
    "GFS_IndicatorsTool>get_Indicators.yml@88|POP_plot": {
      "label": "Map of populations",
      "type": "image/png",
      "weight": 4,
      "description": "Geographic distribution of populations across the study area."
    },
    "GFS_IndicatorsTool>get_Indicators.yml@88|Interactive_plot": {
      "label": "interactive plot",
      "type": "text/html",
      "weight": 5
    }
  }
