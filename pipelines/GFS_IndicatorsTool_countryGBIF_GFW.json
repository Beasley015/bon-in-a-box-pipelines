  "inputs": {
    "pipeline@102": {
      "label": "Title of the run",
      "description": "Set a name for the pipeline run, for instance indicating species of interest, region, type of habitat map, and monitoring period. ",
      "type": "text",
      "example": "Quercus sartorii, Mexico, Habitat decline by tree cover loss, 2000-2023",
      "weight": 0
    },
    "pipeline@105": {
      "label": "Species names",
      "description": "Scientific name of the species, used to look for occurrences in GBIF. ",
      "type": "text",
      "example": "Quercus sartorii",
      "weight": 1
    },
    "pipeline@106": {
      "label": "Countries list",
      "description": "countries of interest, will be used to look for GBIF occurrences.",
      "type": "text[]",
      "example": [
        "Mexico",
        "Guatemala"
      ],
      "weight": 2
    },
    "pipeline@107": {
      "label": "Start year occurrences",
      "description": "Integer, 4 digit year, start date to retrieve occurrences from GBIF",
      "type": "int",
      "example": 1980,
      "weight": 3
    },
    "pipeline@108": {
      "label": "End year occurrences",
      "description": "Integer, 4 digit year, end date to retrieve occurrences from GBIF",
      "type": "int",
      "example": 2000,
      "weight": 4
    },
    "pipeline@113": {
      "label": "Years of monitoring tree cover change",
      "description": "List of years for which tree cover habitat change should be monitored (maximum range 2000 - 2023). The monitoring period should be set after the occurrence retrieval period. ",
      "type": "int[]",
      "example": [
        2000,
        2005,
        2010,
        2015,
        2020
      ],
      "weight": 5
    },
    "pipeline@109": {
      "label": "Size of buffer",
      "description": "Radius size [in km] to calculate buffer around species observations. ",
      "type": "float",
      "example": 10,
      "weight": 6
    },
    "pipeline@110": {
      "label": "Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations. ",
      "type": "float",
      "example": 50,
      "weight": 7
    },
    "pipeline@104": {
      "label": "Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided, the first one will be used in figure.",
      "type": "float[]",
      "example": [
        "0.1",
        "0.2"
      ],
      "weight": 8
    },
    "pipeline@103": {
      "label": "Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided, the first one will be used in figures. ",
      "type": "float[]",
      "example": [
        "50",
        "100",
        "500"
      ],
      "weight": 9
    },
    "pipeline@111": {
      "label": "Resolution of tree cover map",
      "description": "Desired resolution for tree cover map, will be obtained via resampling. To be specified in decimal degrees (0.01 ~ 1 km). Minimal value 0.001 (~100m).",
      "type": "float",
      "example": 0.01,
      "weight": 10
    }
  },
  "outputs": {
    "GFS_Indicators.json@100|GFS_IndicatorsTool>get_Indicators.yml@104|Interactive_plot": {
      "description": "An interactive interface to explore indicators trends across geographical space and time.",
      "label": "Interactive plot",
      "weight": 0,
      "type": "text/html"
    }
  },
  "metadata": {
    "name": "GFS TOOL - Forest cover loss by populations from GBIF occurrences (country)",
    "description": "Genes from Space tool. Retrieves species occurrence from GBIS, calculates polygons of population distribution, and computes indices of genetic diversity monitoring for forest cover habitat loss.",
    "author": [
      {
        "name": "Oliver Selmoni",
        "email": "oliver.selmoni@gmail.com"
      }
    ],
    "external_link": "https://teams.issibern.ch/genesfromspace/ , https://www.globalforestwatch.org/",
    "references": [
      {
        "text": "Schuman et al., EcoEvoRxiv.",
        "doi": "https://doi.org/10.32942/X2RS58"
      },
      {
        "text": "Hansen et al., Science (2013)",
        "doi": "https://doi.org/10.1126/science.1244693"
      }
    ]
  }
