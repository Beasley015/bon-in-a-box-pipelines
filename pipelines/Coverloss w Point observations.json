  "inputs": {
    "pipeline@27": {
      "label": "Buffer",
      "description": "A number indicating the buffer zone around a point observation( in meters)",
      "type": "int",
      "weight": 0,
      "example": 9000
    },
    "pipeline@34": {
      "label": "species names",
      "description": "Scientific name of the species",
      "type": "text",
      "example": "Persea cinerascens",
      "weight": 1
    },
    "pipeline@36": {
      "label": "start year",
      "description": "Integer, 4 digit year, start date to retrieve occurrences",
      "type": "int",
      "example": 1990,
      "weight": 2
    },
    "pipeline@37": {
      "label": "bbox",
      "description": "Vector of float, bbox coordinates of the bbox in the order xmin, ymin, xmax, ymax",
      "type": "float[]",
      "example": [
        "-99",
        "16",
        "-95",
        "21.5"
      ],
      "weight": 3
    },
    "pipeline@38": {
      "label": "end year",
      "description": "Integer, 4 digit year, end date to retrieve occurrences",
      "type": "int",
      "example": 2020,
      "weight": 4
    },
    "pipeline@41": {
      "label": "limit",
      "description": "Integer, maximum number of observations to retrieve from GBIF database (upper limit 100000)",
      "type": "int",
      "example": 2000,
      "weight": 5
    },
    "pipeline@47": {
      "label": "population density",
      "description": "Number describing Population density estimate(Individuals per Km^2)",
      "type": "float",
      "weight": 6
    },
    "pipeline@48": {
      "label": "Ne/Nc ratio",
      "description": "Number from 0 to 1 describing estimated Ne/Nc ratio",
      "type": "float",
      "weight": 7
    }
  },
  "outputs": {
    "Cover loss>loss_map.yml@1|map": {
      "description": "PDF of plotted cover map",
      "label": "Covermap",
      "type": "text/tab-separated-values",
      "weight": 0
    },
    "Cover loss>relative_loss_wcircles.yml@16|relative_loss_wcircles": {
      "description": "plot the relative loss with circles of area",
      "label": "relative loss graph",
      "type": "text/tab-separated-values",
      "weight": 1
    },
    "Cover loss>Indicators.yml@46|Nedata": {
      "description": "Table including Ne estimates(calculated from habitat area, population density a d Ne/Nc ratio), Ne>=500 Indicator and statement if population extinct or not.",
      "label": "Ne estimates",
      "type": "text/tab-separated-values",
      "weight": 2
    },
    "Cover loss>Indicators.yml@46|PMratio": {
      "description": "ratio of populations extinct vs extant( pop. extinct/total nr of pop.)",
      "label": "PM ratio",
      "type": "float",
      "weight": 3
    },
    "Cover loss>Indicators.yml@46|Neratio": {
      "description": "ratio of populations Ne over 500 vs Ne under 500( pop. with Ne>=500/total nr of pop.)",
      "label": "Ne>=500 ratio",
      "type": "float",
      "weight": 4
    }
  }
